# Development defaults
## EXTENSIONS_ENABLED
# Enabled by default (GA). Accepted truthy: 1,true,yes,on,enable,enabled
# Accepted falsy: 0,false,no,off,disable,disabled,''
EXTENSIONS_ENABLED=true

## EXTENSIONS_RUNTIME_ENABLED
# Runtime executor is deferred; keep disabled unless you are explicitly testing no-op wiring.
# Accepted truthy/falsy strings same as above. Default is false if unset.
EXTENSIONS_RUNTIME_ENABLED=false
