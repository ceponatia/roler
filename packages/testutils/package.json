{
  "name": "@roler/testutils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared test utilities for Roler monorepo (Vitest helpers, data builders)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
